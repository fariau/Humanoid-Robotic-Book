{
  "name": "physical-ai-textbook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3003",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ingest": "node backend/rag/ingestion.js",
    "rag:ingest": "node backend/rag/ingestion.js",
    "backend:python": "cd backend && python run_server.py",
    "auth:dev": "cd better-auth-server && npm run dev",
    "dev": "concurrently \"npm run start\" \"npm run backend:python\"",
    "dev:all": "concurrently \"npm run start\" \"npm run backend:python\" \"cd better-auth-server && npx next dev --port 3020\""
  },
  "dependencies": {
    "@docusaurus/core": "3.1.0",
    "@docusaurus/preset-classic": "3.1.0",
    "@langchain/cohere": "^1.0.1",
    "@mdx-js/react": "^3.0.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "better-auth": "^1.4.9",
    "clsx": "^2.0.0",
    "cohere-ai": "^7.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.0",
    "openai": "^4.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.0",
    "@docusaurus/types": "3.1.0",
    "concurrently": "^9.2.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
